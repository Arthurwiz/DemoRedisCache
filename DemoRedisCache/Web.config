<configuration>
  <appSettings>
  <add key="CacheEndpoint" value="yourDNSNameHere.redis.cache.windows.net" />
  <add key="CachePassword" value="YourCachePassword" />
  <add key="CacheSyncTimeout" value="3000" />
  <add key="CacheConnectTimeout" value="4000" />
  </appSettings>
  <system.web>
    <compilation debug="true" targetFramework="4.5" />
    <httpRuntime targetFramework="4.5" />
  </system.web>
</configuration>